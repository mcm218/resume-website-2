{
    "name": "resume-website-2",
    "version": "1.0.9",
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1",
        "dev:app": "npm run config:dev && ng serve",
        "build:api": "tsc -p tsconfig.api.json",
        "start:api": "node ./api/dist/index.js",
        "dev:api": "nodemon --watch api --exec \"NODE_ENV=dev ts-node\" api/index.ts",
        "ng": "ng",
        "start": "ng serve",
        "build": "ng build",
        "config": "NODE_ENV=prod ts-node envLoader.prod.ts",
        "config:dev": "NODE_ENV=dev ts-node envLoader.ts",
        "config:staging": "NODE_ENV=staging ts-node envLoader.ts",
        "config:prod": "NODE_ENV=prod ts-node envLoader.ts",
        "deploy": "npm run config && npm run sentry:sourcemaps & fly deploy",
        "watch": "ng build --watch --configuration development",
        "lint": "ng lint",
        "sentry:sourcemaps": "sentry-cli sourcemaps inject --org personal-03r --project resume-website ./dist/resume-website && sentry-cli sourcemaps upload --org personal-03r --project resume-website ./dist/resume-website",
        "generate": "drizzle-kit generate:sqlite --out api/drizzle/migrations --breakpoints --schema=api/drizzle/schema.ts",
        "migrate": "tsx api/drizzle/migrate"
    },
    "dependencies": {
        "@angular/animations": "~17.0.2",
        "@angular/cdk": "^17.0.0",
